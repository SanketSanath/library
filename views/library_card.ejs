<html>
	<head>
		<title>
			Library card
		</title>
		<script src="https://cdn.jsdelivr.net/npm/jsbarcode@3.8.0/dist/JsBarcode.all.min.js"></script>
		<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
	</head>
	<body>
		<p>
			Name: <%=name%>
		</br>
			Branch: <%=branch%>
		</br>
			Roll: <%=roll%>
		</br>
			Validity: <%=validity%>
		</br>
		<svg id="roll"></svg>
	</p>
	</body>
	<script>
		$(function(){
			JsBarcode("#roll","<%=roll%>");
		})
	</script>
</html>
